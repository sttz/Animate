{
  "metadata": [
    {
      "src": [
        {
          "src": "../Source",
          "files": [
            "**/*.cs"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ]
        }
      ],
      "dest": "obj/temp/api"
    }
  ],
  "build": {
    "content": [
      {
        "src": "obj/temp/api",
        "files": [ "**/*.yml" ],
        "dest": "api"
      },
      {
        "files": [
          "manual/**.md",
          "manual/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appFooter": "Â© 2018 Adrian Stutz",
      "_gitContribute": {
        "repo": "https://github.com/sttz/animate",
        "branch": "master",
        "apiSpecFolder": "docs-ref-overwrite"
      }
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}